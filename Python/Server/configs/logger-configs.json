{
  "logging": {
    "version": 1,
    "formatters": {
      "simple": {
        "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
        "datefmt": "%m/%d/%Y %H:%M:%S"
      }
    },
    "handlers": {
      "console": {
        "class": "logging.StreamHandler",
        "level": "INFO",
        "formatter": "simple",
        "stream": "ext"
      }
    },
    "root": {
      "level": "INFO",
      "handlers": [
        "console"
      ]
    }
  }
}